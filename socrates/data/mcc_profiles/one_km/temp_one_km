   ../../mcc_profiles/one_km/iso_200.q
Csamson2cdl -o iso_200.t -u K -L "TEMPERATURE" -n t \
   ../../mcc_profiles/one_km/iso_200.t
Csamson2cdl -o iso_200.tstar -u K -L "SURFACE TEMPERATURE" -n tstar \
   ../../mcc_profiles/one_km/iso_200.tstar
