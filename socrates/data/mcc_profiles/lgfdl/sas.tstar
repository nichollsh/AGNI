netcdf sas.tstar{                                                               

dimensions:
    lat            =   1;
    lon            =   1;
    plev           =   1;


variables:
    float lat(lat);                                                           
             lat:units = "degree";                                            
             lat:title = "LATITUDE";                                         
    float lon(lon);                                                           
             lon:units = "degree";                                            
             lon:title = "LONGITUDE";                                        
    float plev(plev);                                                         
             plev:units = "Pa";                                               
             plev:title = "PRESSURE";                                        

    float tstar(plev,lon,lat);                                                 
             tstar:units = "K";                                               
             tstar:title = "SURFACE TEMPERATURE";                                            

data:                                                                           
              lat =   .000000E+00;
              lon =   .000000E+00;
             plev =   .101000E+06;
            tstar =   .287000E+03;

}                                                                               
